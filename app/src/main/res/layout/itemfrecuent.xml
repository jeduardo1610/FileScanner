<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="8dp"
    android:layout_marginTop="4dp"
    android:layout_marginBottom="4dp"
    android:id="@+id/relative">



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Extention"
        android:id="@+id/sizeAverage"
        android:layout_marginLeft="53dp"

        android:layout_marginStart="53dp"
        android:layout_marginTop="16dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Occurrences"
        android:id="@+id/occurencesTextView"
        android:layout_marginRight="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="48dp"
        android:layout_alignBaseline="@+id/sizeAverage"
        android:layout_alignBottom="@+id/sizeAverage"
        android:layout_alignRight="@+id/line4"
        android:layout_alignEnd="@+id/line4" />

    <View
        android:id="@+id/line4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        android:layout_below="@id/occurencesTextView"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"/>
</RelativeLayout>